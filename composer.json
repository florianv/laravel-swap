{
    "name": "florianv/laravel-swap",
    "type": "library",
    "description": "Currency exchange rates library for Laravel",
    "keywords": [
        "laravel",
        "currency",
        "money",
        "rate",
        "conversion",
        "exchange rates"
    ],
    "homepage": "https://github.com/florianv/laravel-swap",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Voutzinos",
            "email": "florian@voutzinos.com",
            "homepage": "http://florian.voutzinos.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swap\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swap\\Laravel\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.5|^7.0",
        "florianv/swap": "dev-master"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.1",
        "php-http/guzzle6-adapter": "^1.0",
        "php-http/message": "^1.0",
        "psr/cache": "^1.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
